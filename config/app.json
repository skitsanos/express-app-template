{
  "errors": {
    "logHttpErrors": true,
    "reportErrorsAs": "html",
    "showStack": true
  },
  "bodyLimit": "1mb",
  "parsers": {
    "json": true,
    "urlEncoded": true,
    "forms": true,
    "cookies": true
  },
  "uploads": {
    "storage": "/uploads/",
    "limit": 10485760
  },
  "cms": {
    "enabled": false,
    "mountPath": "/cms"
  },
  "session": {
    "enabled": true,
    "secret": "",
    "resave": false,
    "saveUninitialized": false,
    "cookie": {
      "httpOnly": true,
      "sameSite": "lax",
      "secure": false,
      "maxAge": null
    }
  },
  "strictRouting": false,
  "viewEngine": "hbs",
  "viewExtension": "hbs"
}
